{
  "disabled": false,
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "telegram/{token}/webhook",
      "authLevel": "anonymous",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "entryPoint": "webhook",
  "scriptFile": "../src/functions/telegram/handler.js"
}